ile":"/home/chiboy/lampstack-8.0.5-0/apache2/htdocs/bukassa-web/test/src/components/TextareaField.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"input_field\">\n    <label>{{ label }}</label>\n    <textarea\n        :placeholder=model.placeholder\n        :type=\"model.type\" \n        v-model=\"model.value\" \n        autocomplete=\"false\"\n        :class=\"{\n            input_error: (model.error != '') ? true : false\n        }\"\n        @keyup=\"$emit('input-action')\"\n    ></textarea>\n    <div class=\"error\" v-if=\"model.error != ''\">\n        {{ model.error }}\n    </div>\n  </div>\n</template>\n\n<script>\nimport { defineComponent } from \"@vue/runtime-core\";\n\nexport default defineComponent({\n    name:'textarea-field',\n    props: ['model', 'label'],\n});\n</script>\n\n<style scoped>\n.input_field {\n    display: flex;\n    flex-wrap: wrap;\n    width: 100%;\n    height: 100%;\n    padding: 1% 0;\n}\n.input_field > * {  \n    width: 100%;\n}\nlabel {\n    padding: 1% 0;\n    color: rgb(92, 92, 92);\n}\ntextarea {\n    border: 1px solid grey;\n    padding: 3%;\n    height: 94%;\n    width: 100%;\n    font-size: 100%;\n    border-radius: 5px;\n    outline-color: var(--burgundy-400);\n    resize: none;\n    outline: none;\n}\n.error {\n    color: rgb(201, 60, 60);\n    font-size: 80%;\n}\n.input_error {\n    border: 1px solid rgb(201, 60, 60);\n}\n</style>"]}]}